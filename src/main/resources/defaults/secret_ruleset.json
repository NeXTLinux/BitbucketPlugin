{
  "with_thanks_to": [
    "https://github.com/anshumanbh/git-all-secrets/blob/master/rules.json"
  ],

  "rules": [
    {
      "rule_number": 1,
      "friendly_name": "RSA Private Key",
      "regex_pattern": "-----BEGIN RSA PRIVATE KEY-----.*"
    },
    {
      "rule_number": 2,
      "friendly_name": "Slack Token",
      "regex_pattern": "xox[p|b|o|a].*"
    },
    {
      "rule_number": 3,
      "friendly_name": "PGP Private Key",
      "regex_pattern": "-----BEGIN PGP PRIVATE KEY BLOCK-----.*"
    },
    {
      "rule_number": 4,
      "friendly_name": "Twitter Oauth 2",
      "regex_pattern": "[t|T][w|W][i|I][t|T][t|T][e|E][r|R].*.[0-9a-zA-Z]{35,}"
    },
    {
      "rule_number": 5,
      "friendly_name": "SSH (DSA) private key",
      "regex_pattern": "-----BEGIN DSA PRIVATE KEY-----.*"
    },
    {
      "rule_number": 6,
      "friendly_name": "Facebook Oauth 2",
      "regex_pattern": "[f|F][a|A][c|C][e|E][b|B][o|O][o|O][k|K].*.[0-9a-f]{32,}"
    },
    {
      "rule_number": 7,
      "friendly_name": "Twitter Oauth",
      "regex_pattern": "[t|T][w|W][i|I][t|T][t|T][e|E][r|R].*['|\\\"][0-9a-zA-Z]{35,}['|\\\"]"
    },
    {
      "rule_number": 8,
      "friendly_name": "SSH (EC) private key",
      "regex_pattern": "-----BEGIN EC PRIVATE KEY-----.*"
    },
    {
      "rule_number": 9,
      "friendly_name": "Generic App Secret",
      "regex_pattern": "[a|A][p|P][p|P][s|S][e|E][c|C][r|R][e|E][t|T].*.[0-9a-zA-Z]{32,}"
    },
    {
      "rule_number": 10,
      "friendly_name": "Google Oauth 2",
      "regex_pattern": "[c|C][l|L][i|I][e|E][n|N][T|T][_][s|S][e|E][c|C][r|R][e|E][t|T].*[:].*[a-zA-Z0-9-_]{24,}"
    },
    {
      "rule_number": 11,
      "friendly_name": "Generic App Secret 2",
      "regex_pattern": "[a|A][p|P][p|P][s|S][e|E][c|C][r|R][e|E][t|T].*['|\\\"][0-9a-zA-Z]{32,}['|\\\"]"
    },
    {
      "rule_number": 12,
      "friendly_name": "hunter2 (******)",
      "regex_pattern": "hunter2+"
    },
    {
      "rule_number": 13,
      "friendly_name": "Generic Secret",
      "regex_pattern": "[s|S][e|E][c|C][r|R][e|E][t|T].*['|\\\"][0-9a-zA-Z]['|\\\"]"
    },
    {
      "rule_number": 14,
      "friendly_name": "AWS API Key",
      "regex_pattern": "AKIA[0-9A-Z]{16,}"
    },
    {
      "rule_number": 15,
      "friendly_name": "GitHub Token",
      "regex_pattern": "[g|G][i|I][t|T][h|H][u|U][b|B].*[['|\\\"]0-9a-zA-Z]{35,}['|\\\"]"
    },
    {
      "rule_number": 16,
      "friendly_name": "Generic Private Key",
      "regex_pattern": "-----BEGIN PRIVATE KEY-----."
    },
    {
      "rule_number": 17,
      "friendly_name": "GitHub Token 2",
      "regex_pattern": "[g|G][i|I][t|T][h|H][u|U][b|B].*[c|C][l|L][i|I][e|E][n|N][T|T][s|S][e|E][c|C][r|R][e|E][t|T].*[0-9a-zA-Z]"
    },
    {
      "rule_number": 18,
      "friendly_name": "Google OAuth",
      "regex_pattern": "(\\\"client_secret\\\":\\\"[a-zA-Z0-9-_]{24,}\\\")"
    },
    {
      "rule_number": 19,
      "friendly_name": "Facebook OAuth",
      "regex_pattern": "[f|F][a|A][c|C][e|E][b|B][o|O][o|O][k|K].*['|\\\"][0-9a-f]{32,}['|\\\"]"
    },
     {
      "rule_number": 20,
      "friendly_name": "OpenSSH Private Key",
      "regex_pattern": "-----BEGIN OPENSSH PRIVATE KEY-----.*"
    },
    {
      "rule_number": 21,
      "friendly_name": "Heroku API Key",
      "regex_pattern": "[h|H][e|E][r|R][o|O][k|K][u|U].*[0-9A-F]{8}-[0-9A-F]{4}-[0-9A-F]{4}-[0-9A-F]{4}-[0-9A-F]{12,}"
    },
    {
      "rule_number": 22,
      "friendly_name": "Generic Password",
      "regex_pattern": ".*[p|P][a|A][s|S][s|S][w|W][o|O][r|R][d|D].*=.*"
    }
  ]
}